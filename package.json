{
	"name": "babel-mixin",
	"description": "Babel/Grunt/Mocha/Webpack mixin for running ES2015+ code in Node.js",
	"version": "0.0.5",
	"repository": {
		"type": "git",
		"url": "https://github.com/tom-weatherhead/babel-mixin.git"
	},
	"homepage": "https://github.com/tom-weatherhead/babel-mixin#readme",
	"bugs": {
		"url": "https://github.com/tom-weatherhead/babel-mixin/issues"
	},
	"main": "src/main",
	"scripts": {
		"build": "npm i && grunt",
		"buildw": "npm i & grunt",
		"buildx": "npm i && grunt && npm start",
		"buildwx": "npm i & grunt & npm start",
		"clean": "rm -rf node_modules && rm -f package-lock.json",
		"cleanw": "rmdir /Q /S node_modules & del package-lock.json",
		"rebuild": "npm run clean & npm run build",
		"rebuildw": "npm run cleanw & npm run buildw",
		"lint": "grunt eslint",
		"start": "grunt",
		"test": "grunt"
	},
	"dependencies": {
		"@babel/core": "^7.5.5",
		"@babel/plugin-transform-classes": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/register": "^7.5.5",
		"babel-loader": "^8.0.6",
		"grunt": "^1.0.4",
		"grunt-contrib-concat": "^1.0.1",
		"grunt-eslint": "^22.0.0",
		"grunt-mocha-test": "^0.13.3",
		"grunt-webpack": "^3.1.3",
		"mocha": "^6.2.0",
		"regenerator-runtime": "^0.13.3",
		"thaw-config": "^0.0.4",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"webpack": "^4.39.2"
	},
	"devDependencies": {},
	"engineStrict": true,
	"engines": {
		"node": ">= 8"
	},
	"keywords": [
		"Babel",
		"Grunt",
		"Mocha",
		"Webpack"
	],
	"author": "Tom Weatherhead <null@2hrd4u.org> (https://httpbin.org/status/418)",
	"license": "MIT"
}
